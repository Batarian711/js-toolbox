{
  "name": "@ctrine/toolbox",
  "version": "8.0.0",
  "description": "Tools to develop libraries, apps and websites.",
  "license": "Apache-2.0",
  "bin": {
    "ctr": "build/index.js",
    "ctrine": "build/index.js"
  },
  "scripts": {
    "clean": "rimraf \"build\"",
    "dev": "npm run clean && run-p dev:*",
    "dev:assets": "cpx src/banner/*.txt build/banner",
    "dev:build": "babel src -d build --ignore **/__tests__ --source-maps inline",
    "dev-w": "npm run clean && run-p dev-w:*",
    "dev-w:assets": "cpx src/banner/*.txt build/banner --watch",
    "dev-w:build": "babel src -d build --ignore **/__tests__ --source-maps inline --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrine/toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/ctrine/toolbox/issues"
  },
  "homepage": "http://www.ctrine.com/#/toolbox",
  "keywords": [
    "ctrine",
    "dev",
    "develop",
    "development",
    "tool",
    "toolbox"
  ],
  "author": {
    "name": "Alexandre Borela",
    "email": "alexandre@borela.tech",
    "url": "http://www.borela.tech"
  },
  "dependencies": {
